{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfhandsonmack"
		},
		"Bronze_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Bronze'"
		},
		"Fonte_dados_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Fonte_dados'"
		},
		"Parquet_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Parquet'"
		},
		"gold_parquet_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'gold_parquet'"
		},
		"Bronze_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stghandsonmack.dfs.core.windows.net/"
		},
		"Parquet_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stghandsonmack.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfhandsonmack_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Bronze_accountKey": {
						"value": "[parameters('Bronze_accountKey')]"
					},
					"Fonte_dados_connectionString": {
						"value": "[parameters('Fonte_dados_connectionString')]"
					},
					"Parquet_accountKey": {
						"value": "[parameters('Parquet_accountKey')]"
					},
					"gold_parquet_connectionString": {
						"value": "[parameters('gold_parquet_connectionString')]"
					},
					"Bronze_properties_typeProperties_url": {
						"value": "[parameters('Bronze_properties_typeProperties_url')]"
					},
					"Parquet_properties_typeProperties_url": {
						"value": "[parameters('Parquet_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}